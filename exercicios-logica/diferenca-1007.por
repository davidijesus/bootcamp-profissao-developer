programa {
	funcao inicio() {
	    inteiro A, B, C, D, DIFERENCA
		escreva("Digite o valor de A: \n")
		leia(A)
		escreva("Digite o valor de B: \n")
		leia(B)
		escreva("Digite o valor de C: \n")
		leia(C)
		escreva("Digite o valor de D: \n")
		leia(D)
		
		DIFERENCA = (A * B - C * D)
		escreva("DIFERENCA = ", DIFERENCA)
	}
}
